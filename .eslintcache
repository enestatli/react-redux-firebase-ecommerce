n\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Forms\\Button\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\firebase\\utils.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\firebase\\config.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Signup\\index.js",["264"],"import { useState, useEffect } from 'react';\nimport { useHistory } from 'react-router-dom';\nimport { useDispatch, useSelector } from 'react-redux';\nimport FormInput from '../Forms/FormInput';\nimport Button from '../Forms/Button';\nimport './styles.scss';\n\nimport AuthWrapper from '../AuthWrapper';\n\nimport { signUpUserStart } from '../../redux/User/actions';\n\nconst mapState = ({ user }) => ({\n  currentUser: user.currentUser,\n  userErrors: user.userErrors,\n});\n\nconst Signup = (props) => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const { currentUser, userErrors } = useSelector(mapState);\n\n  const [displayName, setDisplayName] = useState('');\n  const [email, setEmail] = useState('');\n  const [password, setPassword] = useState('');\n  const [confirmPassword, setConfirmPassword] = useState('');\n  const [errors, setErrors] = useState([]);\n\n  useEffect(() => {\n    if (currentUser) {\n      resetForm();\n      history.push('/');\n    }\n  }, [currentUser]);\n\n  useEffect(() => {\n    if (Array.isArray(userErrors) && userErrors.length > 0) {\n      setErrors(userErrors);\n    }\n  }, [userErrors]);\n\n  const resetForm = () => {\n    setDisplayName('');\n    setEmail('');\n    setPassword('');\n    setConfirmPassword('');\n    setErrors([]);\n  };\n\n  const formSubmit = (e) => {\n    e.preventDefault();\n    dispatch(\n      signUpUserStart({ displayName, email, password, confirmPassword })\n    );\n  };\n\n  const configAuthWrapper = {\n    headline: 'Signup',\n  };\n\n  return (\n    <AuthWrapper {...configAuthWrapper}>\n      <div className=\"formWrap\">\n        {errors?.length > 0 && (\n          <ul>\n            {errors.map((err, index) => {\n              return <li key={index}>{err}</li>;\n            })}\n          </ul>\n        )}\n        <form onSubmit={formSubmit}>\n          <FormInput\n            type=\"text\"\n            name=\"displayName\"\n            value={displayName}\n            placeholder=\"Full Name\"\n            handleChange={(e) => setDisplayName(e.target.value)}\n          />\n\n          <FormInput\n            type=\"email\"\n            name=\"email\"\n            value={email}\n            placeholder=\"Email\"\n            handleChange={(e) => setEmail(e.target.value)}\n          />\n\n          <FormInput\n            type=\"text\"\n            name=\"password\"\n            value={password}\n            placeholder=\"Password\"\n            handleChange={(e) => setPassword(e.target.value)}\n          />\n\n          <FormInput\n            type=\"text\"\n            name=\"confirmPassword\"\n            value={confirmPassword}\n            placeholder=\"Confirm Password\"\n            handleChange={(e) => setConfirmPassword(e.target.value)}\n          />\n\n          <Button type=\"submit\">Register</Button>\n        </form>\n      </div>\n    </AuthWrapper>\n  );\n};\n\nexport default Signup;\n","H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Forms\\FormInput\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\pages\\Recovery\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\AuthWrapper\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\EmailPassword\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\createStore.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\rootReducer.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\User\\reducer.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\User\\types.js",[],"H:\\Projects\\react-redux-firebase-ecommer