eSubmit = (e) => {\n    e.preventDefault();\n    dispatch(signInUser({ email, password }));\n  };\n\n  const configAuthWrapper = {\n    headline: 'Login',\n  };\n\n  return (\n    <AuthWrapper {...configAuthWrapper}>\n      <div className=\"formWrap\">\n        <form onSubmit={handleSubmit}>\n          <FormInput\n            type=\"email\"\n            name=\"email\"\n            value={email}\n            placeholder=\"Email\"\n            handleChange={(e) => setEmail(e.target.value)}\n          />\n\n          <FormInput\n            type=\"text\"\n            name=\"password\"\n            value={password}\n            placeholder=\"Password\"\n            handleChange={(e) => setPassword(e.target.value)}\n          />\n\n          <Button type=\"submit\">Log In</Button>\n\n          <div className=\"socialSignin\">\n            <div className=\"row\">\n              <Button onClick={signInWithGoogle}>Sign in with Google</Button>\n            </div>\n          </div>\n\n          <div className=\"links\">\n            <Link to=\"/recovery\">Reset Password</Link>\n          </div>\n        </form>\n      </div>\n    </AuthWrapper>\n  );\n};\n\nexport default withRouter(SignIn);\n","H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Forms\\Button\\index.js",[],["138","139"],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\firebase\\utils.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\firebase\\config.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Signup\\index.js",["140"],"import { useState, useEffect } from 'react';\nimport { withRouter } from 'react-router-dom';\nimport { useDispatch, useSelector } from 'react-redux';\nimport FormInput from '../Forms/FormInput';\nimport Button from '../Forms/Button';\nimport './styles.scss';\n\nimport AuthWrapper from '../AuthWrapper';\n\nimport { signupUser } from '../../redux/User/actions';\n\nconst mapState = ({ user }) => ({\n  signUpSuccess: user.signUpSuccess,\n  signUpEr