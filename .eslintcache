me, productPrice } = product;\n\n          if (\n            !productThumbnail ||\n            !productName ||\n            typeof productPrice === 'undefined'\n          )\n            return null;\n\n          const configProduct = {\n            ...product,\n          };\n\n          return <Product {...configProduct} key={index} />;\n        })}\n      </div>\n\n      {!isLastPage && <LoadMore {...configLoadMore} />}\n    </div>\n  );\n};\n\nexport default ProductResults;\n","H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\ProductResults\\Product\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\LoadMore\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\pages\\ProductDetails\\index.js",["324"],"import ProductCard from '../../components/ProductCard';\n\nconst ProductDetails = ({}) => {\n  return (\n    <div>\n      <ProductCard />\n    </div>\n  );\n};\n\nexport default ProductDetails;\n","H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\ProductCard\\index.js",["325"],"import { useEffect } from 'react';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { useHistory, useParams } from 'react-router-dom';\nimport { addProduct } from '../../redux/Cart/actions';\nimport { fetchProductStart, setProduct } from '../../redux/Products/actions';\n\nimport Button from './../../components/Forms/Button';\nimport './styles.scss';\n\nconst ProductCard = () => {\n  const dispatch = useDispatch();\n  const history = useHistory();\n  const product = useSelector((state) => state.productsData.product);\n  const { productID } = useParams();\n  const { productName, productThumbnail, productPrice, productDesc } = product;\n\n  useEffect(() => {\n    dispatch(fetchProductStart(productID));\n\n    return () => {\n      dispatch(setProduct({}));\n    };\n  }, []);\n\n  const configAddtoCartBtn = {\n    type: 'button',\n  };\n\n  const handleAddToCart = (product) => {\n    if (!product) return;\n    dispatch(addProduct(product));\n    history.push('/cart');\n  };\n\n  return (\n    <div className=\"productCard\">\n      <div className=\"hero\">\n        <img src={productThumbnail} alt={productName} />\n      </div>\n\n      <div className=\"productDetails\">\n        <ul>\n          <li>\n            <h1>{productName}</h1>\n          </li>\n\n          <li>\n            <h1>${productPrice}</h1>\n          </li>\n\n          <li>\n            <div className=\"addToCart\">\n              <Button\n                {...configAddtoCartBtn}\n                onClick={() => handleAddToCart(product)}\n              >\n                Add to cart\n              </Button>\n            </div>\n          </li>\n\n          <li>\n            <span dangerouslySetInnerHTML={{ __html: productDesc }}></span>\n          </li>\n        </ul>\n      </div>\n    </div>\n  );\n};\n\nexport default ProductCard;\n","H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\Cart\\reducer.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\Cart\\types.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\Cart\\actions.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\Cart\\utils.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\redux\\Cart\\selectors.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\pages\\Cart\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Checkout\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Checkout\\Item\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\pages\\Payment\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\PaymentDetails\\index.js",["326"],"import { CountryDropdown } from 'react-country-region-selector';\nimport { CardElement, useElements, useStripe } from '@stripe/react-stripe-js';\n\nimport FormInput from './../Forms/FormInput';\nimport Button from './../Forms/Button';\nimport { useEffect, useS