ayout>\n          )}\n        />\n        <Route\n          path=\"/product/:productID\"\n          render={() => (\n            <MainLayout>\n              <ProductDetails />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/registration\"\n          render={() => (\n            <MainLayout>\n              <Registration />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/login\"\n          render={() => (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/recovery\"\n          render={() => (\n            <MainLayout>\n              <Recovery />\n            </MainLayout>\n          )}\n        />\n        <Route\n          path=\"/dashboard\"\n          render={() => (\n            <WithAuth>\n              <MainLayout>\n                <Dashboard />\n              </MainLayout>\n            </WithAuth>\n          )}\n        />\n        <WithAdminAuth>\n          <Route\n            path=\"/admin\"\n            render={() => (\n              <AdminLayout>\n                <Admin />\n              </AdminLayout>\n            )}\n          />\n        </WithAdminAuth>\n      </Switch>\n    </div>\n  );\n};\n\nexport default App;\n","H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Header\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\pages\\Homepage\\index.js",[],"H:\\Projects\\react-redux-firebase-ecommerce\\src\\components\\Directory\\index.js",["230","231"],"import './styles.scss';\nimport ShopMen from '../../assets/shopMens.jpg';\nimport ShopWomen from '../../assets/shopWomens.jpg';\n\nconst Directory = (props) => {\n  return (\n    <div className=\"directory\">\n      <div className=\"wrap\">\n        <div\n          className=\"item\"\n          style={{\n            backgroundImage: `url(${ShopWomen})`,\n          }}\n        >\n          <a>Shop Womens</a>\n     